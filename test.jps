  - set:
      multiRegions: false

  - script: |
      return { 
        result: 0, 
        count_regions: '${settings.regions}'.split(',').length
      }
  - if ( '${response.count_regions}' == 2 ):
      set:
        multiRegions: true

  - script: |
      return { 
        result: 0, 
        regions: '${settings.regions}'.split(',')
      }

  - forEach(response.regions):
      log: --- ${@i}  -   ${@}
      
  - if ( ${this.multiRegions} ):
      log: ---zxczxczxczxczxc-
